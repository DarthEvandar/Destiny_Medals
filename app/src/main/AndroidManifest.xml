<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.anders.destinymedals" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon4"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <receiver android:name="com.apps.anders.destinymedals.Alarming" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllMedals"
            android:label="@string/title_activity_receive" >
        </activity>
        <activity
            android:name=".SettingsClass"
            android:label="@string/title_activity_settings_class" >
        </activity>
        <activity
            android:name=".Cached"
            android:label="@string/title_activity_cached" >
        </activity>
    </application>

</manifest>
